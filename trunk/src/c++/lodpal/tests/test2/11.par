  base_factor=.4
  region=11
}
 

#parameters for LODPAL

lodpal_analysis, out=one
{
  trait=sle
  autosomal
  {
    model=one_parameter
    parent_of_origin=true
  }
  covariate=race_aa
  diagnostic="11s1984"
}

lodpal_analysis, out=two
{
  trait=sle
  autosomal
  {
    model=two_parameter
    parent_of_origin=true
  }
  covariate=race_aa
  diagnostic="11s1984"
}

