pedigree
{
  pedigree_id=PID
  individual_id=ID
  parent_id=P1
  parent_id=P2
  sex_field=SEX
  marker=M0, missing="?/?"
  marker=M1, missing="?/?"
  marker=M2, missing="?/?"
  marker=M3, missing="?/?"
  marker=M4, missing="?/?"  

  delimiters=" "
  delimiter_mode=multiple
  individual_missing_value=0
  verbose=20
}

decipher
{
  epsilon=.000000001
  region=all, first="M4", last="M0"

  data
  {
    analysis_unit=each_individual
  }
  
  filters
  {
    maf_filter=true
  }

  tasks
  {
    pop_freq=true;
  }
}

